matrix1 = [
    [1, 2],
    [3, 4]
]
matrix2 = [
    [5, 6],
    [7, 8]
]
result = []
for i in range(len(matrix1)):
    row = []
    for j in range(len(matrix1[0])):
        row.append(matrix1[i][j] + matrix2[i][j])
    result.append(row)
print("\nResultant Matrix after Addition:")
for row in result:
    print(row)

#OUTPUT
Resultant Matrix after Addition:
[6, 8]
[10, 12]
